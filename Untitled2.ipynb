    "def printPairs(arr, n, sum):\n",
    " \n",
    "  \n",
    "    for i in range(0, n):\n",
    "        for j in range(i + 1, n):\n",
    "            if (arr[i] + arr[j] == sum):\n",
    "                print(arr[i], arr[j], sep=\" \")\n",
    "                     \n",
    " \n",
    " \n",
    " \n",
    "arr = [1, 5, 7, -1, 5]\n",
    "n = len(arr)\n",
    "sum = 6\n",
    "printPairs(arr, n, sum)"
